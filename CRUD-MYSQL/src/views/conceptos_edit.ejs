<%- include ('partials/_header') %>

<div class="container mt-5">
    <div class="row">
      <div class="col-md-5 offset-md-3 card">
        <div class="card-body">
          <form action="/update/<%= data.id %>" method="POST">
            <div class="form-group">
              <input type="text" name="concepto" value="<%= data.concepto%>" placeholder="Concepto" class="form-control" autofocus>
            </div>
            <div class="form-group">
              <input type="text" name="indicador" value="<%= data.indicador%>" placeholder="Indicador" class="form-control">
            </div>
  
            <button type="submit" class="btn btn-primary">
              save concepto
            </button>
          </form>
        </div>
      </div>
    </div>
  </div>
  

<%- include ('partials/_footer') %>