<%- include ('partials/_header') %>

<div class="container mt-5">
    <div class="row">
      <div class="col-md-5 offset-md-3 card">
        <div class="card-body">
          <form action="/update/<%= data.id %>" method="POST">
            <div class="form-group">
              <input type="text" name="id_definicion" value="<%= data.id_definicion%>" placeholder="Id_Definicion" class="form-control" autofocus>
            </div>
            <div class="form-group">
              <input type="text" name="id_tipoactivo" value="<%= data.id_tipoactivo%>" placeholder="Id_TipoActivo" class="form-control">
            </div>
            <div class="form-group">
              <input type="text" name="descripcion" value="<%= data.descripcion%>" placeholder="Descripcion" class="form-control">
            </div>
            <div class="form-group">
                <input type="text" name="url" value="<%= data.url%>" placeholder="Url" class="form-control">
            </div>
  
            <button type="submit" class="btn btn-primary">
              save activo
            </button>
          </form>
        </div>
      </div>
    </div>
  </div>
  

<%- include ('partials/_footer') %>