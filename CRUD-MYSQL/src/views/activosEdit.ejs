<%- include ('partials/_header') %>

<div class="container mt-5">
    <div class="row">
      <div class="col-md-5 offset-md-3 card">
        <div class="card-body">
          <form action="/activos/update/<%= data.id %>" method="POST">
            <div class="form-group">
              <input type="text" name="idDefinicion" value="<%= data.idDefinicion%>" placeholder="IdDefinicion" class="form-control" autofocus>
            </div>
            <div class="form-group">
              <input type="text" name="idTipoActivo" value="<%= data.idTipoActivo%>" placeholder="IdTipoActivo" class="form-control">
            </div>
            <div class="form-group">
                <input type="text" name="descripcion" value="<%= data.descripcion%>" placeholder="Descripcion" class="form-control">
            </div>
            <div class="form-group">
                <input type="text" name="url" value="<%= data.url%>" placeholder="Url" class="form-control">
            </div>
  
            <button type="submit" class="btn btn-primary">
              save Activo
            </button>
          </form>
        </div>
      </div>
    </div>
  </div>
  

<%- include ('partials/_footer') %>