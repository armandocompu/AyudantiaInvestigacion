<%- include ('partials/_header') %>

<div class="container mt-5">
    <div class="row">
      <div class="col-md-5 offset-md-3 card">
        <div class="card-body">
          <form action="/definiciones/update/<%= data.id %>" method="POST">
            <div class="form-group">
              <input type="text" name="id_fuente" value="<%= data.id_fuente%>" placeholder="IdFuente" class="form-control" autofocus>
            </div>
            <div class="form-group">
              <input type="text" name="id_concepto" value="<%= data.id_concepto%>" placeholder="IdConcepto" class="form-control">
            </div>
            <div class="form-group">
                <input type="text" name="definicion" value="<%= data.definicion%>" placeholder="Definicion" class="form-control">
            </div>
            <div class="form-group">
                <input type="text" name="fecha" value="<%= data.fecha%>" placeholder="Fecha" class="form-control">
            </div>
  
            <button type="submit" class="btn btn-primary">
              save Definicion
            </button>
          </form>
        </div>
      </div>
    </div>
  </div>
  

<%- include ('partials/_footer') %>