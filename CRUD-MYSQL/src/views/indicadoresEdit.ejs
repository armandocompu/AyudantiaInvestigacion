<%- include ('partials/_header') %>

<div class="container mt-5">
    <div class="row">
      <div class="col-md-5 offset-md-3 card">
        <div class="card-body">
          <form action="indicadores/update/<%= data.id %>" method="POST">
            <div class="form-group">
              <input type="text" name="id_definicion" value="<%= data.id_definicion%>" placeholder="Id_definicion" class="form-control" autofocus>
            </div>
            <div class="form-group">
              <input type="text" name="id_programa" value="<%= data.id_programa%>" placeholder="Id_programa" class="form-control">
            </div>
            <div class="form-group">
                <input type="text" name="tipo" value="<%= data.tipo%>" placeholder="Tipo" class="form-control">
            </div>
            <div class="form-group">
                <input type="text" name="unidad" value="<%= data.unidad%>" placeholder="Unidad" class="form-control">
            </div>
  
            <button type="submit" class="btn btn-primary">
              save indicador
            </button>
          </form>
        </div>
      </div>
    </div>
  </div>
  

<%- include ('partials/_footer') %>