<%- include ('partials/_header') %>

<div class="container mt-5">
    <div class="row">
      <div class="col-md-5 offset-md-3 card">
        <div class="card-body">
          <form action="fuentes/update/<%= data.id %>" method="POST">
            <div class="form-group">
              <input type="text" name="fuente" value="<%= data.fuente%>" placeholder="Fuente" class="form-control" autofocus>
            </div>
            <div class="form-group">
              <input type="text" name="origen" value="<%= data.origen%>" placeholder="Origen" class="form-control">
            </div>
            <div class="form-group">
                <input type="text" name="tipo" value="<%= data.tipo%>" placeholder="Tipo" class="form-control">
            </div>
            <div class="form-group">
                <input type="text" name="url" value="<%= data.url%>" placeholder="Url" class="form-control">
            </div>
  
            <button type="submit" class="btn btn-primary">
              save Fuente
            </button>
          </form>
        </div>
      </div>
    </div>
  </div>
  

<%- include ('partials/_footer') %>