<%- include ('partials/_header') %>

<div class="container mt-5">
    <div class="row">
      <div class="col-md-5 offset-md-3 card">
        <div class="card-body">
          <form action="sinonimos/update/<%= data.id %>" method="POST">
            <div class="form-group">
              <input type="text" name="id_concepto" value="<%= data.id_concepto%>" placeholder="Concepto" class="form-control" autofocus>
            </div>
            <div class="form-group">
              <input type="text" name="sinonimo" value="<%= data.sinonimo%>" placeholder="Sinonimo" class="form-control">
            </div>
            <div class="form-group">
                <input type="text" name="observacion" value="<%= data.observacion%>" placeholder="Observacion" class="form-control">
            </div>
            <button type="submit" class="btn btn-primary">
              save Sinonimo
            </button>
          </form>
        </div>
      </div>
    </div>
  </div>
  

<%- include ('partials/_footer') %>